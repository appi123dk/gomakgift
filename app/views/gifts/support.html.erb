<% content_for :javascript do %>

<script type="text/javascript">


$(function() {
  $('.accordion-toggle').click(function(){
    $('.accordion-toggle').css({"background-color" : "rgba(0,0,0,0.8)", "color" : "white", "font-weight": "300"});
    $('.iconBlock').css({"background-color" : "transparent", "color" : "white"});
    $(this).css({"background-color" : "#fed328", "color" : "#000000", "font-weight":"500"});
    $(this).children('.iconBlock').css({"background-color" : "#fed328", "color" : "#000000", "font-weight":"500"});
  });
});

</script>

<% end %>